<script>

var vid = document.getElementById("scroll-video");

if (window.matchMedia('(prefers-reduced-motion)').matches) {
	
    vid.removeAttribute("autoplay");
    vid.pause();
}


vid.addEventListener('ended', function()
{
	vid.pause();
	vidFade();
}); 

vid.addEventListener("click", function() {
  if (vid.paused) {
    playVideo();
  } else {
    pauseVideo();
  }
})

					$('#scroll-video').bind('inview', function (event, visible) {
                            if (visible) {								
                                playVideo();
                            } else {								
                                pauseVideo();
                            }
                        });

function playVideo(){vid.play();vid.classList.remove("stopfade");}
function pauseVideo(){vid.pause();vid.classList.add("stopfade");}



